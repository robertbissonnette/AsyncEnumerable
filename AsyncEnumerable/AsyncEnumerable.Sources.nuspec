<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>AsyncEnumerable.Sources</id>
    <version>1.0.0.0</version>
    <description>
		Allows for easier creation of IAsyncEnumerable supporting yield return and the such.  
		This class was taken from http://idcomlog.codeplex.com/ and packaged as a source only file to include in projects requiring it.
    </description>
    <authors>Robert Bissonnette,iD Commerce + Logistics</authors>
    <language>en-US</language>
    <projectUrl>https://github.com/robertbissonnette/AsyncEnumerable</projectUrl>
    <licenseUrl>http://idcomlog.codeplex.com/license</licenseUrl>
    <contentFiles>
      <files include="cs/any/*.*" buildAction="Compile" />
	</contentFiles>
	<dependencies> 
		<dependency id="System.Interactive.Async" version="3.0.0" />
	</dependencies>
  </metadata>
  <files>
	<file src="*.cs" target="contentFiles\cs\any" />
  </files>
</package>